{
  "host": "mongodb.localdomain",
  "port": 27017,
  "name": "fireworks",
  "authsource": "$external",
  "logdir": null,
  "strm_lvl": "ERROR",
  "mongoclient_kwargs": {
    "tls": true,
    "tlsCAFile": "/path/to/cacert.pem",
    "tlsCertificateKeyFile": "/path/to/usercertkey.pem",
    "tlsCertificateKeyFilePassword": "private key passphrase",
    "authMechanism": "MONGODB-X509"
  }
}
