{
  "host": "mongodb.localdomain",
  "port": 27017,
  "name": "fireworks",
  "authsource": "$external",
  "logdir": null,
  "strm_lvl": "ERROR",
  "ssl": true,
  "ssl_ca_certs": "/path/to/cacert.pem",
  "ssl_certfile": "/path/to/usercert.pem",
  "ssl_keyfile": "/path/to/userkey.pem",
  "ssl_pem_passphrase": "private key passphrase",
  "mongoclient_kwargs": {
    "authmechanism": "MONGODB-X509"
  }
}