{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/IntegerArray",
  "IntegerArray": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{integer_array.IntegerArray}}"
      },
      "data": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      },
      "reftest": {
        "$ref": "refschema.json#/RefTest"
      }
    },
    "required": [
      "_fw_name"
    ]
  }
}
